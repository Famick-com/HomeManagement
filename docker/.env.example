# Famick Home Management - Docker Environment Variables
# Copy this file to .env and update the values

# Database Configuration
DB_PASSWORD=changeme123

# JWT Authentication
# IMPORTANT: Change this to a random string of at least 32 characters
JWT_SECRET_KEY=your-secret-key-change-this-min-32-characters-long

# HTTPS Certificate Password
CERT_PASSWORD=password

# pgAdmin (optional - only used with --profile tools)
PGADMIN_EMAIL=admin@local.dev
PGADMIN_PASSWORD=admin
